{
  "__collections__": {
    "users": {
      "__doc__": {
        "id": "string - User ID (matches Firebase Auth UID)",
        "email": "string - User email address",
        "role": "string - 'Administrator' | 'Auditor' | 'Viewer'",
        "createdAt": "timestamp - Account creation date",
        "updatedAt": "timestamp - Last profile update"
      },
      "__example__": {
        "id": "user_001",
        "email": "admin@proveniq.io",
        "role": "Administrator",
        "createdAt": "2024-12-11T00:00:00Z",
        "updatedAt": "2024-12-11T00:00:00Z"
      }
    },
    "partners": {
      "__doc__": {
        "id": "string - Partner ID",
        "name": "string - Company name",
        "type": "string - 'CARRIER' | 'TPA' | 'SOFTWARE_VENDOR' | 'OTHER'",
        "status": "string - 'ACTIVE' | 'SUSPENDED' | 'INACTIVE'",
        "dataResidency": "string - 'us-east1' | 'eu-west1' | 'ap-southeast1'",
        "createdAt": "timestamp - Onboarding date",
        "updatedAt": "timestamp - Last update"
      },
      "__example__": {
        "id": "partner_acme_insurance",
        "name": "Acme Insurance Co.",
        "type": "CARRIER",
        "status": "ACTIVE",
        "dataResidency": "us-east1",
        "createdAt": "2024-12-11T00:00:00Z",
        "updatedAt": "2024-12-11T00:00:00Z"
      }
    },
    "claims": {
      "__doc__": {
        "id": "string - Claim ID (e.g., 'CLM-2024-0751')",
        "timestamp": "timestamp - Block creation time",
        "hash": "string - SHA-256 hash of block contents",
        "previousHash": "string - Hash of previous block (chain linkage)",
        "caseId": "string (optional) - Related case grouping"
      },
      "__example__": {
        "id": "CLM-2024-0751",
        "timestamp": "2024-07-28T10:05:00Z",
        "hash": "a1b2c3d4e5f6...",
        "previousHash": "0000000000000000...",
        "caseId": "CASE-HUR-24-01"
      },
      "__subcollections__": {
        "transactions": {
          "__doc__": {
            "id": "string - Transaction ID (e.g., 'T001')",
            "timestamp": "timestamp - When action occurred",
            "user": "string - Actor (username or 'System')",
            "action": "string - 'CLAIM_CREATED' | 'DOCUMENT_UPLOADED' | 'ASSESSMENT_COMPLETE' | 'PAYMENT_ISSUED' | etc.",
            "details": "string - Human-readable description"
          },
          "__example__": {
            "id": "T001",
            "timestamp": "2024-07-28T10:00:00Z",
            "user": "System",
            "action": "CLAIM_CREATED",
            "details": "Initial claim filed for policy #P12345."
          }
        }
      }
    },
    "ledgerEvents": {
      "__doc__": {
        "id": "string - UUID",
        "eventType": "string - 'CLAIM_CREATED' | 'CLAIM_UPDATED' | 'CLAIM_VERIFIED' | 'CLAIM_FLAGGED' | 'CLAIM_CLOSED' | 'DOCUMENT_ATTACHED' | 'DOCUMENT_VERIFIED' | 'PROOF_GENERATED' | 'RISK_ASSESSED' | 'HUMAN_OVERRIDE' | 'DATA_ERASURE'",
        "sequence": "number - Sequential order in chain",
        "previousHash": "string - Chain linkage",
        "hash": "string - Event hash",
        "payload": "map - Event-specific data (varies by eventType)",
        "payloadHash": "string - Hash of payload",
        "timestamp": "timestamp - Event time",
        "partnerId": "string - Insurance carrier ID",
        "actorId": "string - User who triggered event",
        "signature": "string - Ed25519 signature",
        "publicKeyId": "string - Signing key reference",
        "dataResidency": "string - 'us-east1' | 'eu-west1' | 'ap-southeast1'",
        "retentionPolicy": "string - Data retention rule"
      },
      "__example__": {
        "id": "evt_abc123",
        "eventType": "CLAIM_CREATED",
        "sequence": 1,
        "previousHash": "0000000000000000...",
        "hash": "sha256_hash_here",
        "payload": {
          "claimId": "CLM-2024-0751",
          "partnerClaimId": "ACME-12345",
          "claimType": "AUTO",
          "policyNumber": "P12345",
          "claimAmount": 5000,
          "currency": "USD"
        },
        "payloadHash": "payload_sha256_hash",
        "timestamp": "2024-07-28T10:00:00Z",
        "partnerId": "partner_acme_insurance",
        "actorId": "user_001",
        "signature": "ed25519_signature",
        "publicKeyId": "key_001",
        "dataResidency": "us-east1",
        "retentionPolicy": "7_YEARS"
      }
    },
    "anomalies": {
      "__doc__": {
        "id": "string - Anomaly ID",
        "blockId": "string - Reference to claim",
        "reason": "string - Anomaly description",
        "detectedAt": "timestamp - Detection time",
        "status": "string - 'OPEN' | 'REVIEWED' | 'DISMISSED'",
        "reviewedBy": "string (optional) - User who reviewed"
      },
      "__example__": {
        "id": "anomaly_001",
        "blockId": "CLM-2024-0752",
        "reason": "Unusual claim amount for policy type",
        "detectedAt": "2024-07-28T12:00:00Z",
        "status": "OPEN",
        "reviewedBy": null
      }
    },
    "fraudScores": {
      "__doc__": {
        "id": "string - Score ID",
        "blockId": "string - Reference to claim",
        "score": "number - 0-100 risk score",
        "level": "string - 'Low' | 'Medium' | 'High'",
        "justification": "string - AI explanation",
        "assessedAt": "timestamp - Assessment time",
        "modelVersion": "string - AI model version used"
      },
      "__example__": {
        "id": "score_001",
        "blockId": "CLM-2024-0752",
        "score": 75,
        "level": "High",
        "justification": "Multiple claims from same address in 30 days",
        "assessedAt": "2024-07-28T12:05:00Z",
        "modelVersion": "fraud_model_v2.1"
      }
    },
    "annotations": {
      "__doc__": {
        "id": "string - Annotation ID",
        "transactionId": "string - Reference to transaction",
        "user": "string - Author email",
        "timestamp": "timestamp - When added",
        "comment": "string - Annotation text"
      },
      "__example__": {
        "id": "AN-1702300000000",
        "transactionId": "T003",
        "user": "auditor@proveniq.io",
        "timestamp": "2024-12-11T15:00:00Z",
        "comment": "Verified document authenticity with carrier."
      }
    },
    "complianceViolations": {
      "__doc__": {
        "id": "string - Violation ID",
        "transactionId": "string - Reference to transaction",
        "ruleId": "string - Compliance rule ID",
        "message": "string - Violation description",
        "severity": "string - 'LOW' | 'MEDIUM' | 'HIGH' | 'CRITICAL'",
        "detectedAt": "timestamp - Detection time",
        "resolvedAt": "timestamp (optional) - Resolution time",
        "resolvedBy": "string (optional) - User who resolved"
      },
      "__example__": {
        "id": "violation_001",
        "transactionId": "T005",
        "ruleId": "GDPR-ART17",
        "message": "PII retention exceeds allowed period",
        "severity": "HIGH",
        "detectedAt": "2024-12-11T10:00:00Z",
        "resolvedAt": null,
        "resolvedBy": null
      }
    },
    "auditLog": {
      "__doc__": {
        "id": "string - Log entry ID",
        "timestamp": "timestamp - Event time",
        "user": "string - Actor email",
        "action": "string - Action performed",
        "details": "string - Action details",
        "ipAddress": "string (optional) - Client IP",
        "userAgent": "string (optional) - Client user agent"
      },
      "__example__": {
        "id": "log_001",
        "timestamp": "2024-12-11T15:30:00Z",
        "user": "admin@proveniq.io",
        "action": "BLOCK_VIEWED",
        "details": "Viewed details for block CLM-2024-0751",
        "ipAddress": "192.168.1.1",
        "userAgent": "Mozilla/5.0..."
      }
    },
    "riskAssessments": {
      "__doc__": {
        "id": "string - Assessment ID",
        "claimId": "string - Reference to claim",
        "assessmentId": "string - Unique assessment identifier",
        "modelVersion": "string - AI model version",
        "fraudScore": "number - 0-100 score",
        "riskLevel": "string - 'LOW' | 'MEDIUM' | 'HIGH'",
        "confidenceScore": "number - 0-1 confidence",
        "factors": "array - Risk factors with contributions",
        "recommendation": "string - 'APPROVE' | 'REVIEW' | 'INVESTIGATE' | 'DENY'",
        "humanReviewRequired": "boolean - Needs human review",
        "assessedAt": "timestamp - Assessment time",
        "humanOverride": "map (optional) - Override details if reviewed"
      },
      "__example__": {
        "id": "assessment_001",
        "claimId": "CLM-2024-0752",
        "assessmentId": "RA-2024-001",
        "modelVersion": "risk_model_v3.0",
        "fraudScore": 65,
        "riskLevel": "MEDIUM",
        "confidenceScore": 0.85,
        "factors": [
          {
            "name": "claim_frequency",
            "contribution": 0.4,
            "description": "3 claims in past 90 days",
            "category": "PATTERN"
          }
        ],
        "recommendation": "REVIEW",
        "humanReviewRequired": true,
        "assessedAt": "2024-07-28T12:10:00Z",
        "humanOverride": null
      }
    },
    "verificationProofs": {
      "__doc__": {
        "id": "string - Proof ID",
        "proofType": "string - 'EVENT_EXISTENCE' | 'CLAIM_HISTORY' | 'DOCUMENT_AUTHENTICITY' | 'RANGE_INTEGRITY'",
        "generatedAt": "timestamp - Generation time",
        "subject": "map - { type, id, hash }",
        "merkleProof": "map - { root, rootTimestamp, path[] }",
        "chainContext": "map - { firstEventSequence, lastEventSequence, chainLength }",
        "signature": "string - Proof signature",
        "signerKeyId": "string - Signing key ID",
        "verificationEndpoint": "string - URL to verify"
      },
      "__example__": {
        "id": "proof_001",
        "proofType": "EVENT_EXISTENCE",
        "generatedAt": "2024-12-11T16:00:00Z",
        "subject": {
          "type": "EVENT",
          "id": "evt_abc123",
          "hash": "event_hash_here"
        },
        "merkleProof": {
          "root": "merkle_root_hash",
          "rootTimestamp": "2024-12-11T15:59:00Z",
          "path": [
            { "hash": "sibling_hash", "position": "LEFT", "level": 0 }
          ]
        },
        "chainContext": {
          "firstEventSequence": 1,
          "lastEventSequence": 100,
          "chainLength": 100
        },
        "signature": "proof_signature",
        "signerKeyId": "key_001",
        "verificationEndpoint": "https://api.proveniq.io/verify"
      }
    },
    "apiKeys": {
      "__doc__": {
        "id": "string - Key ID",
        "partnerId": "string - Owner partner ID",
        "keyPrefix": "string - First 8 chars of key (for display)",
        "keyHash": "string - SHA-256 hash of full key",
        "name": "string - Key name/description",
        "scopes": "array - Allowed API scopes",
        "status": "string - 'ACTIVE' | 'REVOKED'",
        "createdAt": "timestamp - Creation time",
        "lastUsedAt": "timestamp (optional) - Last usage",
        "expiresAt": "timestamp (optional) - Expiration time"
      },
      "__example__": {
        "id": "key_001",
        "partnerId": "partner_acme_insurance",
        "keyPrefix": "pk_live_",
        "keyHash": "sha256_of_full_key",
        "name": "Production API Key",
        "scopes": ["read:claims", "write:claims", "read:proofs"],
        "status": "ACTIVE",
        "createdAt": "2024-12-01T00:00:00Z",
        "lastUsedAt": "2024-12-11T15:00:00Z",
        "expiresAt": "2025-12-01T00:00:00Z"
      }
    }
  }
}
